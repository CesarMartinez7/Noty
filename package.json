{
  "name": "Notys",
  "displayName": "Notys",
  "description": "Notys es una extensi√≥n ligera y pr√°ctica para Visual Studio Code que te permite crear, organizar y gestionar notas directamente desde tu editor. Ideal para desarrolladores que quieren tener apuntes r√°pidos, recordatorios o snippets siempre a la mano sin salir del flujo de trabajo.",
  "version": "1.0.8",
  "icon": "./assets/secrets.png",
  "publisher": "Develoops",
  "homepage": "https://github.com/CesarMartinez7/SECRETS-NOTE.git",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/CesarMartinez7/SECRETS-NOTE.git"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vscodeSidebar.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "Notys üìù - Bloc de Notas.",
          "icon": "assets/secrets.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "vscodeSidebar.openview",
          "name": "View",
          "contextualTitle": "View"
        }
      ]
    },
    "commands": [
      {
        "command": "vscodeSidebar.menu.view",
        "category": "vscode-extension-sidebar-html",
        "title": "Sample WebView in VS Code Sidebar",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscodeSidebar.menu.view",
          "group": "navigation",
          "when": "view == vscodeSidebar.openview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0"
  }
}
